{
  "name": "context_and_custom_hooks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "postcss src/index.css -o output.css",
    "watch:css": "postcss src/index.css -o output.css -w",
    "dev": "vite & tailwindcss -i ./src/input.css  -o ./dist/output.css --watch",
    "start": "npm run watch:css & react-scripts start",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "typescript": "^4.9.3",
    "tailwindcss": "^3.2.7",
    "vite": "^4.0.0"
  }
}
